  rdfs:domain :Sources ;
  rdfs:range :Source ;
.
:hasSources
  a owl:ObjectProperty ;
  rdfs:domain :Prediction ;
  rdfs:range :Sources ;
.
:hasSpectra
  a owl:ObjectProperty ;
