:intensity
  a owl:DatatypeProperty ;
  rdfs:domain :PeakValue ;
  rdfs:range xsd:decimal ;
.
:modificationMass
  a owl:DatatypeProperty ;
  rdfs:domain :Modification ;
  rdfs:label "The mass change that would occur due to the modification. E.g.:, phosphorylation introduces a mass shift of +79.99 Dalton. This can also be a negative value, if needed."@en ;
  rdfs:range xsd:decimal ;
.
:modificationName
  a owl:DatatypeProperty ;
  rdfs:domain :Modification ;
  rdfs:label "The name given to a particular modification. E.g., phosphorylation."@en ;
  rdfs:range xsd:string ;
.
:monoIsotopicMass
  a owl:DatatypeProperty ;
  rdfs:domain :ModifiedAminoAcid ;
  rdfs:label "The mono isotopic mass of an element e.g., an amino acid. Tryptophan: 186.08."@en ;
